import socket
import os

host = '127.0.0.1'
port = 12345
shared = "sharing"
filelist = os.listdir(shared)
print filelist

sockfd = socket.socket()

sockfd.connect((host, port))

fread = open("testfile",'rb')

line = fread.read(1024)

while line:
	data = sockfd.send(line)
	line = fread.read(1024)

fread.close()
sockfd.close()

print data

